{
  "name": "smurl-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node --unhandled-rejections=strict ./dist/app.js",
    "debug": "export DEBUG=* && yarn run start",
    "dev": "concurrently \"tsc --watch\" \"nodemon -q ./dist/app.js\"",
    "test": "mocha -r ts-node/register 'test/**/*.test.ts' --unhandled-rejections=strict",
    "test-debug": "export DEBUG=* && npm test"
  },
  "dependencies": {
    "argon2": "^0.29.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.3",
    "shortid": "^2.2.16",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.23",
    "@types/shortid": "^0.0.29",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.6",
    "concurrently": "^7.4.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  }
}
